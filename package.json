{
  "name": "postgres-prisma",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build-db && npm run seed-db && next build",
    "build-db": "prisma generate && prisma db push",
    "seed-db": "prisma db seed",
    "build-sql": "npx prisma migrate dev --name init",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/async": "^3.2.24",
    "async": "^3.2.5",
    "bcrypt": "^5.1.1",
    "iron-session": "^8.0.1",
    "lodash-es": "^4.17.21",
    "next": "14.0.4",
    "prisma": "^5.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "bootstrap": "^5.3.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "8.4.32",
    "prettier": "^3.1.1",
    "puppeteer": "^21.3.6",
    "sass": "^1.69.5",
    "ts-node": "^10.9.2",
    "type-fest": "^4.9.0",
    "typescript": "5.3.3"
  },
  "type": "module"
}