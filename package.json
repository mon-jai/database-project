    "lodash-es": "^4.17.21",
    "ms": "^2.1.3",
    "next": "13.5.4",
    "postcss": "8.4.31",
    "prisma": "^5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
